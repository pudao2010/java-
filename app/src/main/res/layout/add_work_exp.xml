<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.qhzk.ciep.widget.MineItemView
        android:id="@+id/entry_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemKey="入职时间" />

    <com.qhzk.ciep.widget.MineItemView
        android:id="@+id/deputare_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemKey="离职时间" />

    <com.qhzk.ciep.widget.MineItemEdit
        android:id="@+id/company"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemEditHint="请输入"
        app:itemEditKey="公    司" />

    <com.qhzk.ciep.widget.MineItemView
        android:id="@+id/industry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemKey="行    业" />

    <com.qhzk.ciep.widget.MineItemView
        android:id="@+id/unit_size"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemKey="公司规模" />

    <com.qhzk.ciep.widget.MineItemView
        android:id="@+id/unit_nature"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemKey="公司性质" />

    <com.qhzk.ciep.widget.MineItemEdit
        android:id="@+id/job_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemEditHint="请输入"
        app:itemEditKey="职位名称" />

    <com.qhzk.ciep.widget.MineItemEdit
        android:id="@+id/department"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemEditHint="请输入"
        app:itemEditKey="部    门" />

    <com.qhzk.ciep.widget.MineItemView
        android:id="@+id/job_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemKey="工作类型" />
</merge>