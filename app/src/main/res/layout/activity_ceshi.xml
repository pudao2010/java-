<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_ceshi"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.qhzk.ciep.widget.CeshiActivity">

    <com.qhzk.ciep.widget.MineItemView
        android:id="@+id/birthday"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemKey="中文姓名"
        app:itemValue="张学友" />

    <com.qhzk.ciep.widget.MineItemView
        android:id="@+id/country"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemKey="英文姓名"
        app:itemValue="zahgnxueyou" />

    <com.qhzk.ciep.widget.MineItemEdit
        android:id="@+id/desc"
        app:itemEditKey="自我描述"
        app:itemEditHint="请输入描述"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>
